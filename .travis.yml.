language: python

python:
  - 3.7

matrix:
  include:
    - python: 3.7
      dist: xenial
      sudo: true
      
before_script:
  - export FLASK_APP="$TRAVIS_BUILD_DIR/autoapp.py"
  - export FLASK_DEBUG=1
  - cd install_dependencies
  - python install_dependencies.py
  - cd ..
  
script:
  - python run.py

